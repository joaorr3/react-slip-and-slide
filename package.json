{
  "name": "react-slip-and-slide",
  "version": "3.4.11",
  "license": "MIT",
  "scripts": {
    "bundle": "npx nx run react-slip-and-slide:esbuild",
    "build": "npx nx run-many --target=build --exclude root web-app native-app",
    "postbuild": "npm run bundle",
    "version": "npx nx run root:version --syncVersions --exclude=root,web-app,native-app",
    "publish": "npx nx run-many --target=publish --exclude root web-app native-app",
    "web": "npx nx run web-app:serve",
    "web:build": "npx nx run web-app:build-app",
    "clear": "watchman watch-del-all && rm -rf $TMPDIR/metro-cache",
    "native:start": "npx nx run native-app:start",
    "native:ios": "npx nx run native-app:run-ios",
    "native:android": "npx nx run native-app:run-android",
    "patch-package": "patch-package",
    "postinstall": "npm run patch-package"
  },
  "private": true,
  "dependencies": {
    "@use-gesture/react": "^10.2.26",
    "core-js": "^3.30.1",
    "dequal": "^2.0.3",
    "immer": "^10.0.1",
