    "patch-package": "^7.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-is": "^18.0.0",
    "react-native": "0.71.4",
    "react-native-gesture-handler": "^2.11.0",
    "react-spring": "9.6.1",
    "rxjs": "^7.8.0",
    "styled-components": "^5.3.10"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
